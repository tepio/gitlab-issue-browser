{
    "name": "hatch/gitlab-issue-browser",
    "description": "A simple gitlab issue browser. Helpful to show backlog issues to clients",
    "license": "MIT",
    "type": "project",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Ryan Schumacher",
            "email": "ryan@38pages.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "silex/silex": "~1.0",
        "silex/web-profiler": "~1.0",
        "symfony/browser-kit": "~2.3",
        "symfony/class-loader": "~2.3",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/css-selector": "~2.3",
        "symfony/debug": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/form": "~2.3",
        "symfony/monolog-bridge": "~2.3",
        "symfony/process": "~2.3",
        "symfony/security": "~2.3",
        "symfony/translation": "~2.3",
        "symfony/twig-bridge": "~2.3",
        "symfony/validator": "~2.3",
        "m4tthumphrey/php-gitlab-api": "~6.0",
        "jrschumacher/silex-provider-gitlab": "~0.1",
        "vlucas/phpdotenv": "~1.0"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
